char *screen = 0x3001;

putchar(char c) {
    *screen = c;
    screen = screen + 1;
}

putstr(char* s) {
    while (*s) {
        putchar(*s);
        s = s + 1;
    }
}

main() {
    putchar('H');
    putchar('e');
    putchar('l');
    putchar('l');
    putchar('o');

    screen = 0x3000 + 40 * 2;
    putstr("Hello world!");
    putstr("Test");
}
