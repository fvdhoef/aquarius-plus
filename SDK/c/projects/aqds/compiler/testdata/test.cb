#asm
    include "../../../../../../../EndUser/sdcard/sdk/asm/inc/basic_stub.inc"
main:
    jp      _main

_putchar:
    push    ix
    ld      ix,0
    add     ix,sp
    ld      a,(ix+4)
    call    $1D94
    ld      sp,ix
    pop     ix
    ret

#endasm

putstr(char* s) {
    while (*s) {
        putchar(*s);
        s = s + 1;
    }
}

main() {
    putstr("Hello world!");
    putstr("Test");
}
